pod 'Masonry'#自动约束
pod 'AFNetworking'#网络框架
pod 'MBProgressHUD' #提示框
pod 'TZImagePickerController'  #第三方本地图片引用
pod 'RSKImageCropper' #第三方本地图片裁剪
pod 'MJRefresh' #第三方下拉刷新
pod 'IQKeyboardManager' #键盘自动管理
pod 'JSONModel'
pod 'SDWebImage' #第三方图片加载
pod 'UMCCommon' #友盟统计
pod 'UMCAnalytics'  
pod 'UMCCommonLog'  
pod 'UMCSecurityPlugins' 
pod 'UMCShare/UI' # 集成微信(完整版14.4M)
pod 'UMCShare/Social/WeChat' # 集成QQ/QZone/TIM(完整版7.6M)
pod 'UMCShare/Social/QQ' # 集成QQ/QZone/TIM(完整版7.6M)
pod 'JPush' #极光推送
